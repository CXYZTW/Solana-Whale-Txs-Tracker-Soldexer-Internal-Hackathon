# Telegram Bot Token (required)
# Get this from @BotFather on Telegram
TELEGRAM_BOT_TOKEN=

# SQD API Configuration
SQD_API_URL=https://portal.sqd.dev/datasets/solana-mainnet

# Whale Detection Threshold (default for new users)
# Amount in SOL or USD based on WHALE_THRESHOLD_TYPE
WHALE_THRESHOLD=50000

# Whale Threshold Type: 'SOL' or 'USD' (default for new users)
WHALE_THRESHOLD_TYPE=USD

# Alert Settings
ENABLE_ALERTS=true
ALERT_COOLDOWN_MINUTES=5

# Environment
NODE_ENV=production
LOG_LEVEL=info