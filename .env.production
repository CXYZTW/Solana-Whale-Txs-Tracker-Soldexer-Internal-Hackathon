# Production Environment Variables for Railway
NODE_ENV=production

# Required: Telegram Bot Token from @BotFather
TELEGRAM_BOT_TOKEN=

# Solana API Configuration
SQD_API_URL=https://portal.sqd.dev/datasets/solana-mainnet

# Default Whale Detection Settings
WHALE_THRESHOLD=50000
WHALE_THRESHOLD_TYPE=USD

# Data Refresh Settings (1-300 seconds)
DEFAULT_POLLING_INTERVAL_SECONDS=15

# Alert Configuration
ENABLE_ALERTS=true
ALERT_COOLDOWN_MINUTES=5